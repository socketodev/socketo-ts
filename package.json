{
  "name": "@socketo/sdk",
  "type": "module",
  "version": "0.0.1-alpha.0",
  "author": "eckoln",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --clean",
    "format": "bunx biome format --write ./src/*"
