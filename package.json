      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "browser": false,
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
